<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated codesinfosOpt -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="codesinfos.presentation.CodesinfosModelWizardID"
            name="%_UI_CodesinfosModelWizard_label"
            class="codesinfos.presentation.CodesinfosModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/CodesinfosModelFile.gif">
         <description>%_UI_CodesinfosModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated codesinfosOpt -->
      <editor
            id="codesinfos.presentation.CodesinfosEditorID"
            name="%_UI_CodesinfosEditor_label"
            icon="icons/full/obj16/CodesinfosModelFile.gif"
            extensions="codesinfos"
            class="codesinfos.presentation.CodesinfosEditor"
            contributorClass="codesinfos.presentation.CodesinfosActionBarContributor">
      </editor>
   </extension>

</plugin>
